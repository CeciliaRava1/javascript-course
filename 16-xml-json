// XML FIle
<person>
    <name>Marcos</name>
    <age>18</age>
    <dateOfBirth>01/09/20</dateOfBirth>
    <programmingLanguages>python, rust</programmingLanguages>
</person>

// JSON FIle
{
    "name": "Luna",
    "age": 30,
    "dateOfBirth": "21/01/98",
    "programmingLanguages": ["go", "typescript"]
}

